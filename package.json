      "pre-commit": "lint-staged",
      "commit-msg": "bun run commitlint --edit $1"
    }
  },
  "engines": {
    "node": "^18.17.1",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "eslint": "^9.7.0",
    "husky": "^9.1.1",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.617.0",
    "@aws-sdk/client-kinesis": "^3.616.0",
    "@aws-sdk/client-sqs": "^3.616.0"
  }
