{
  "name": "mqueue-node",
  "version": "1.0.0",
  "description": "Multiple queue for node client",
  "main": "dist/lib.js",
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint ./src/**/*.ts"
  },
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "src/**/*.{ts}": [
      "eslint --fix"
    ]
